{
  "name": "yemowith-contracts",
  "version": "1.2.0",
  "description": "yemowith contract, include all tools you need to build a dapps or wallets project ",
  "main": "index.js",
  "bin": {
    "yemowith-contracts": "./bin/yemowith-contracts.js",
    "types:generate": "forge build --out ./build/out && npx typechain --target ethers-v6 --out-dir build/Interfaces 'build/out/**/*.json'"
  },
  "files": [
    "/contracts/**/*.sol",
    "/build/**/*.json",
    "!/mocks/**/*"
  ],
  "scripts": {
    "test": "hardhat test",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js",
    "coverage": "hardhat coverage",
    "lint": "prettier --check 'contracts/**/*.sol' 'test/**/*.ts'",
    "lint:fix": "prettier --write 'contracts/**/*.sol' 'test/**/*.ts'",
    "clean": "rm -rf build && rm -rf cache",
    "format": "prettier --write 'contracts/**/*.sol' 'test/**/*.ts'",
    "types:generate": "forge build --out ./build/out &&  npx typechain --target ethers-v6 --out-dir build/Interfaces 'build/out/**/*.json'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yemowith/yemowith-contracts.git"
  },
  "keywords": [
    "yemowith",
    "contracts",
    "dapp",
    "tools"
  ],
  "author": "yemowith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yemowith/yemowith-contracts/issues"
  },
  "homepage": "https://github.com/yemowith/yemowith-contracts#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "consola": "^3.2.3",
    "hardhat": "^2.22.3",
    "hardhat-interface-generator": "^0.0.6",
    "prettier-plugin-solidity": "^1.3.1",
    "typechain": "^8.3.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2",
    "truffle": "^5.11.5"
  }
}
